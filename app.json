{
    "expo": {
        "name": "rn-app",
        "slug": "rn-app",
        "version": "1.0.0",
        "orientation": "portrait",
        "scheme": "rnapp",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "permissions": [
                "android.permission.INTERNET"
            ],
            "adaptiveIcon": {
                "backgroundColor": "#ffffff"
            },
            "edgeToEdgeEnabled": true,
            "package": "com.personal.app"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#ffffff"
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        }
    }
}